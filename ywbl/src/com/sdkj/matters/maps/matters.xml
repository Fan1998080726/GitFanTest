 		 insert into matters
   (id, inittime, updatetime, matter_type, supervision_matter, main_task, 
   supervision_code, matter_source, charge_lead, chaosong_lead, source_unit, 
   cooperate_unit, matters_id, end_time, fankui_time, fid, handover_person,
    handover_tel, breaks,sendtime,state,feed_type,warn_time)
 values
   (#id#,#inittime#,#updatetime#,#matter_type#,#supervision_matter#,
   #main_task#,#supervision_code#,#matter_source#,#charge_lead#,
   #chaosong_lead#,#source_unit#,#cooperate_unit#,#matters_id#,
   #end_time#,#fankui_time#,#fid#,#handover_person#,#handover_tel#,
   #breaks#,#sendtime#,#state#,#feed_type#,#warn_time#) 


	</update>

	<update	id="updateMatterData"		parameterClass="mattersVo"		>
  update matters
    set
        updatetime =#updatetime#,
        matter_type =#matter_type#,
        supervision_matter =#supervision_matter#,
        main_task =#main_task#,
        supervision_code =#supervision_code#,
        matter_source =#matter_source#,
        charge_lead =#charge_lead#,
        chaosong_lead =#chaosong_lead#,
        source_unit =#source_unit#,
        cooperate_unit =#cooperate_unit#,
        end_time =#end_time#,
        fankui_time =#fankui_time#,
        handover_person =#handover_person#,
        handover_tel =#handover_tel#,
        breaks =#breaks#,
        state =#state#,
        sendtime=#sendtime#
        ,feed_type=#feed_type#
        ,warn_time=#warn_time#
  where  
          id =#id# 
	</update>

